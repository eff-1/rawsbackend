# Server Configuration
PORT=5000
NODE_ENV=development

# Google Custom Search API (Primary Image Search)
# Get these from: https://console.developers.google.com/
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_custom_search_engine_id_here

# Backup Image APIs (when Google quota is exceeded)
UNSPLASH_ACCESS_KEY=your_unsplash_access_key_here
PEXELS_API_KEY=your_pexels_api_key_here

# Firebase Configuration
FIREBASE_API_KEY=your_firebase_api_key
FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
FIREBASE_PROJECT_ID=your_project_id
FIREBASE_STORAGE_BUCKET=your_project.appspot.com
FIREBASE_MESSAGING_SENDER_ID=your_sender_id
FIREBASE_APP_ID=your_app_id
FIREBASE_DATABASE_URL=https://your_project.firebaseio.com

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Pinterest API (Optional - Legacy)
PINTEREST_API_KEY=your_pinterest_api_key
PINTEREST_ACCESS_TOKEN=your_access_token

# WhatsApp Business Number (in international format)
BUSINESS_WHATSAPP_NUMBER=2348128653553